<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.malicia.mrg.mvc.controllers.mainFrameController">
    <children>
        <MenuBar prefHeight="0.0" prefWidth="640.0" VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="_New"/>
                        <MenuItem mnemonicParsing="false" onAction="#selectFichierLigthroom" text="Open…"/>
                        <Menu mnemonicParsing="false" text="_Open Recent"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem mnemonicParsing="false" text="_Close"/>
                        <MenuItem mnemonicParsing="false" text="_Save"/>
                        <MenuItem mnemonicParsing="false" text="_Save As…"/>
                        <MenuItem mnemonicParsing="false" text="_Revert"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem mnemonicParsing="false" text="_Preferences…"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem mnemonicParsing="false" text="_Quit"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Actions">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#deleteEmptyDirectory" text="Purge Rep Vide"/>
                        <MenuItem mnemonicParsing="false" onAction="#movenewtogrpphotos" text="move New To GrpPhotos"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem mnemonicParsing="false" text="_Cut"/>
                        <MenuItem mnemonicParsing="false" text="_Copy"/>
                        <MenuItem mnemonicParsing="false" text="_Paste"/>
                        <MenuItem mnemonicParsing="false" text="_Delete"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem mnemonicParsing="false" text="_Select All"/>
                        <MenuItem mnemonicParsing="false" text="_Unselect All"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem fx:id="about" mnemonicParsing="false" onAction="#abouturl" text="About PhotoTri2"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="93.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
            <children>
                <Label fx:id="databaselrcat" alignment="CENTER" layoutX="15.0" layoutY="6.0" prefHeight="0.0"
                       prefWidth="611.0" style="&#10;" text="Drag components from Library here…" textAlignment="CENTER"
                       textFill="#9f9f9f" wrapText="false"/>
                <ChoiceBox fx:id="rootSelected" layoutX="15.0" layoutY="23.0" prefWidth="150.0"/>
                <Label fx:id="lbTempsAdherence" layoutX="15.0" layoutY="65.0" prefHeight="17.0" prefWidth="235.0"
                       text="Label"/>
                <Label fx:id="lbRepertoireNew" layoutX="15.0" layoutY="48.0" prefHeight="17.0" prefWidth="235.0"
                       text="Label"/>
                <CheckBox fx:id="chkDryRun" layoutX="177.0" layoutY="27.0" mnemonicParsing="false"
                          onAction="#ChangeDryRun" text="DryRun"/>
            </children>
        </AnchorPane>
        <HBox prefHeight="100.0" prefWidth="200.0"/>
    </children>
</VBox>
